<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>    
    <TargetFrameworks>netcoreapp3;netcoreapp3.1;net5.0</TargetFrameworks>    
    <Authors>Daniel Oliveira</Authors>    
    <PackageLicenseExpression>MIT</PackageLicenseExpression>    
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>    
    <RepositoryUrl>https://github.com/SpinnerAlloc/Spinner</RepositoryUrl>    
    <PackageIcon>svgexport-6.png</PackageIcon>    
    <PackageIconUrl />    
    <PackageTags>Integration Lagacy Comunication</PackageTags>    
    <PackageProjectUrl>https://github.com/SpinnerAlloc/Doc</PackageProjectUrl>    
    <Description />
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.VisualStudio.CoreUtility" Version="17.0.487" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\Assets\svgexport-6.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
